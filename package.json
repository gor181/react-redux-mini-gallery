{
  "name": "simpixelated-starter",
  "version": "0.0.1-beta2",
  "description": "A boilerplate starter kit for React projects using ES6, Babel, ESLint, and Webpack.",
  "main": "index.js",
  "scripts": {
    "prestart": "npm install",
    "start": "webpack-dev-server --config ./tooling/webpack.config.js --inline --quiet",
    "lint": "eslint ./src",
    "clean": "rm -rf ./dist/public",
    "prebuild": "npm run clean",
    "build": "webpack --config ./tooling/webpack.config.js --progress --colors --bail"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simpixelated/react-es6-starter.git"
  },
  "keywords": [
    "react",
    "es6",
    "es2015",
    "babel",
    "webpack",
    "boilerplate",
    "starter"
  ],
  "author": "Jordan \"Simpixelated\" Kohl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simpixelated/react-es6-starter/issues"
  },
  "homepage": "https://github.com/simpixelated/react-es6-starter#readme",
  "dependencies": {
    "lodash": "4.0.1",
    "moment": "2.11.2",
    "react": "0.14.7",
    "react-dom": "0.14.7"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-core": "6.7.2",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "6.6.1",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "css-loader": "0.23.1",
    "eslint": "2.2.0",
    "eslint-plugin-lodash": "1.2.1",
    "eslint-plugin-react": "4.1.0",
    "html-webpack-plugin": "2.9.0",
    "style-loader": "0.13.0",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1"
  }
}
